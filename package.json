{
  "name": "meeting-bot-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"npm run start\" \"npm run backend\"",
    "backend": "./start-backend.sh",
    "backend:docker": "cd backend && docker-compose up --build",
    "backend:docker:down": "cd backend && docker-compose down",
    "backend:install": "cd backend && python3 -m venv venv && source venv/bin/activate && pip install -r requirements-compatible.txt",
    "fullstack": "concurrently \"npm run start\" \"npm run backend:docker\"",
    "build:all": "npm run build && cd backend && docker build -t meeting-bot-backend ."
  },
  "dependencies": {
    "@types/node": "^16.18.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.0",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "safari >= 9"
    ]
  }
}
